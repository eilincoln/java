package executable.javacourse;

public class FazendoOperacoesEntreNumerosInteiros2 {
	public static void main(String[] args) {
		
		int nota1 = 90;
		int nota2 = 70;
		int nota3 = 50;
		int nota4 = 88;
		
		double mediaFinal = (nota1 + nota2 + nota3 + nota4) / 4;
		
		System.out.println("Sua média é: " + mediaFinal);
		
	}
}